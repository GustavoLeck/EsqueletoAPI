<!DOCTYPE html>
	<html lang="pt-br">
		<head>
			<meta charset="utf-8"/>
			<title>Notícias</title>
		</head>
	<body>

		<h1>Notícias</h1>

		<table>
			
			<tr>
				<td>ID</td>
				<td>Código</td>
				<td>Bot</td>
				<td>Nome</td>
				<td>Autor</td>
				<td>Data</td>
				<td>Hora</td>
				<td>Descrição</td>
			</tr>
			<% for(var i = 0; i < musica.length; i++){ %>
				<tr>
					<td><%= musica[i].ID %></td>
					<td><%= musica[i].CODIGO %></td>
					<td><%= musica[i].BOT %></td>
					<td><%= musica[i].NOME %></td>
					<td><%= musica[i].CANAL %></td>
					<td><%= musica[i].DURACAO %></td>
					<td><%= musica[i].VIZUALIZACAO %></td>
					<td><%= musica[i].LINK %></td>		
                    <td><%= musica[i].DATE%></td>	
                    <td><%= musica[i].TIME %></td>				
				</tr>
			<% } %>

		</table>

	</body>
</html>